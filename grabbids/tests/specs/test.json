{
  "constants": {
    "base_dir": "ds105",
    "derivs_dir": "{base_dir}/derivs",
  },
  "entities": [
    {
      "name": "subject",
      "pattern": "(sub-\\d+)",
      "directory": "({base_dir}/{subject})"
    },
    {
      "name": "run",
      "pattern": "(run-\\d+)"
    },
    {
      "name": "session",
      "pattern": "(ses-\\d)",
      "mandatory": false,
      "missing-value": "func"
    },
    {
      "name": "type",
      "pattern": ".*_(.*?)\\."
    },
    {
        "name": "realignment-param",
        "pattern": ""
    }
  ],
  "result": [
    "subject",
    "session",
    "run",
    "type"
  ]
}