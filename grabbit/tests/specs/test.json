{
  "entities": [
    {
      "name": "subject",
      "pattern": "(sub-\\d+)",
      "directory": "({base_dir}/{subject})"
    },
    {
      "name": "session",
      "pattern": "(ses-\\d)",
      "mandatory": false,
      "missing_value": "ses-1"
    },
    {
      "name": "run",
      "pattern": "(run-\\d+)"
    },
    {
      "name": "type",
      "pattern": ".*_(.*?)\\."
    },
    {
      "name": "bval",
      "pattern": "(.*\\.bval)"
    }
  ],
  "hierarchy": [
    "subject",
    "session",
    "run",
    "type"
  ]
}